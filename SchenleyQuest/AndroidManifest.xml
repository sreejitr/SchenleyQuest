<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.schenleyquest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.schenleyquest.Main"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.schenleyquest.Questions"
            android:label="@string/title_activity_questions" >
        </activity>
        <activity
            android:name="com.example.schenleyquest.Options"
            android:label="@string/title_activity_options" >
        </activity>
        <activity
            android:name="com.example.schenleyquest.Hints"
            android:label="@string/title_activity_hints" >
        </activity>
        <activity
            android:name="com.example.schenleyquest.TransitionScreen"
            android:label="@string/title_activity_transition_screen" >
        </activity>
        <activity
            android:name="com.example.schenleyquest.WinScreen"
            android:label="@string/title_activity_win_screen" >
        </activity>
    </application>

</manifest>
